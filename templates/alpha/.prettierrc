{
    "parser": "typescript",
    "overrides": [
        {
            "files": "*.cjs",
            "options": { "parser": "typescript" }
        },
        {
            "files": "*.json",
            "options": { "parser": "json" }
        },
        {
            "files": "*.css",
            "options": { "parser": "css" }
        }
    ]
}
